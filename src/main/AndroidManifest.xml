<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="nora.honeyexpress">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_bee_launcher_new"
        android:roundIcon="@mipmap/ic_bee_launcher_new"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:background="#ffe082">
        <activity android:name=".Shop"
            android:parentActivityName=".Our_Products"/>
        <activity android:name=".Our_Products"
            android:parentActivityName=".FullscreenActivity"/>
        <activity android:name=".Infromation"
            android:parentActivityName=".FullscreenActivity"/>
        <activity
            android:name=".FullscreenActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/app_name"
            android:theme="@style/FullscreenTheme"
            android:background="#ffe082">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>

</manifest>